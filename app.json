{
  "expo": {
    "name": "Life",
    "slug": "mylife-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151421"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5596a83c-661a-4477-806f-ee4c8a125f7e"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "20251304",
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.dmq.mylifemobile"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#151421"
      },
      "permissions": ["android.permission.RECORD_AUDIO", "android.permission.USE_BIOMETRIC", "android.permission.USE_FINGERPRINT"],
      "package": "com.dmq.mylifemobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0d0f14",
          "image": "./assets/images/icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5596a83c-661a-4477-806f-ee4c8a125f7e"
      }
    },
    "owner": "dmq",
    "runtimeVersion": "1.0.0"
  }
}
